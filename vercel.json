{
    "builds": [{
        "src": "calldata/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python5.0.1" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "calldata/wsgi.py"
        }
    ]
}
